{
  "name": "uninstaller",
  "productName": "uninstaller",
  "description": "uninstaller",
  "keywords": [
    "ХНУ",
    "Машинобудування",
    "Університет"
  ],
  "main": "./main.js",
  "version": "1.0.0",
  "author": "Суручан Володимир|ХНУ",
  "scripts": {
    "start": "electron .",
    "build-all": "electron-builder --ia32 --x64 -w && C: && dir/s",
    "build-win32": "electron-builder --ia32 -w",
    "build-win64": "electron-builder --x64 -w"
  },
  "build": {
    "extraResources": [
      "DataBase"
    ],
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "uninstallDisplayName": "JSLearn.js",
      "deleteAppDataOnUninstall": true,
      "createDesktopShortcut": "always",
      "createStartMenuShortcut": true,
      "menuCategory": true
    },
    "protocols": {
      "name": "khnu_mechanic-protocol",
      "schemes": [
        "khnu_mechanic"
      ]
    }
  },
  "dependencies": {
    "@electron/remote": "2.0.9",
    "fs-extra": "^11.1.1",
    "fswin": "^3.23.311"
  },
  "devDependencies": {
    "electron": "^22.1.0",
    "electron-builder": "^24.4.0"
  }
}